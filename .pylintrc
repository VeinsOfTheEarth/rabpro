
[BASIC]

# mark pdb commands as bad-names
bad-names=a,
        alias,
        args,
        b,
        break,
        bt,
        w,
        c,
        cl,
        clear,
        commands,
        condition,
        cont,
        continue,
        d,
        debug,
        disable,
        display,
        down,
        enable,
        EOF,
        exit,
        q,
        h,
        help,
        ignore,
        interact,
        j,
        jump,
        l,
        list,
        ll,
        longlist,
        n,
        next,
        p,
        pp,
        quit,
        r,
        restart,
        return,
        retval,
        run,
        rv,
        s,
        source,
        step,
        tbreak,
        u,
        unalias,
        undisplay,
        unt,
        until,
        up,
        w,
        whatis,
        where
          
[MESSAGES CONTROL]

disable=C0103,
        W0102,
        R1721,
        C0301,
        E1101,
        W0719,
        W3101,
        C0114,
        C0302,
        W0718,
        C0702
